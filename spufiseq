-----------------------------------------------------------------------
-- Create the IBM Text Search routines
-----------------------------------------------------------------------
 CREATE PROCEDURE SYSPROC.SYSTS_ALTER
   ( IN INDEXSCHEMA            VARCHAR(128)
   , IN INDEXNAME              VARCHAR(128)
   , IN OPTIONS                VARCHAR(32000)
   )
   PARAMETER CCSID UNICODE
   EXTERNAL NAME DSN5OPAL
   DYNAMIC RESULT SETS 0
   LANGUAGE C
   PARAMETER STYLE SQL
   MODIFIES SQL DATA
   COLLID SYSIBMTS
   STAY RESIDENT NO
   SECURITY USER
   PROGRAM TYPE MAIN
   RUN OPTIONS 'TRAP(OFF), HEAP(,,ANY),BELOW(4K,,),ALL31(ON),
                STACK(,,ANY,),POSIX(ON),XPLINK(ON)'
   COMMIT ON RETURN NO
   INHERIT SPECIAL REGISTERS
   WLM ENVIRONMENT DSNWLM_GENERAL ;

 CREATE PROCEDURE SYSPROC.SYSTS_CREATE
   ( IN    INDEXSCHEMA         VARCHAR(128)
    ,IN    INDEXNAME           VARCHAR(128)
    ,IN    TEXTSOURCE          VARCHAR(1024)
    ,IN    OPTIONS             VARCHAR(32000)
   )
   PARAMETER CCSID UNICODE
   EXTERNAL NAME DSN5OPCR
   DYNAMIC RESULT SETS 0
   LANGUAGE C
   PARAMETER STYLE SQL
   MODIFIES SQL DATA
   COLLID SYSIBMTS
   STAY RESIDENT NO
   SECURITY USER
   PROGRAM TYPE MAIN
   RUN OPTIONS 'TRAP(OFF), HEAP(,,ANY),BELOW(4K,,),ALL31(ON),
                STACK(,,ANY,),POSIX(ON),XPLINK(ON)'
   COMMIT ON RETURN NO
   INHERIT SPECIAL REGISTERS
   WLM ENVIRONMENT DSNWLM_GENERAL ;

 CREATE PROCEDURE SYSPROC.SYSTS_DROP
