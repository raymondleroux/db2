./ ADD NAME=PSSHUNL1
SYS3.AMIOPS.V202204.APSSSAMP(PSSHUNL1)                                  00010000
*********************************************************************** 00020000
*  This member contains the UNLOAD statements, which unload table rows* 00030000
*  from a DB2 subsystem.  The table rows must meet the following      * 00040000
*  criteria from the DB2 subsystem:                                   * 00050000
*  OBJOWNR = '<extract_creator>'                                      * 00060000
*                                                                     * 00070000
*  Change the tokens in brackets (<>) to match your installation      * 00080000
*  before submitting PSSHUNLD:                                        * 00090000
*                                                                     * 00100000
*  <pss_tb_qual>         Qualifier of BMC SQL Explorer history tables * 00110000
*                        to be unloaded                               * 00120000
*                                                                     * 00130000
*  <extract_creator>     A unique tag to identify records to be       * 00140000
*                        copied to another subsystem.  This is the    * 00150000
*                        value stored as OBJOWNR in the product       * 00160000
*                        history tables when the job PSSEXTRJ was run.* 00170000
*                        The same value should be used when running   * 00180000
*                        PSSCOMPJ.                                    * 00190000
*                                                                     * 00200000
*********************************************************************** 00210000
-UNLD 001010 SELECT * FROM  <pss_tb_qual>.SQLX_BASE                     00220000
               WHERE OBJOWNR = '<extract_creator>'                      00230000
             ;                                                          00240000
             DDNAME SYSRE001                                            00250000
-SYNC 001020 UNLOAD OF TABLE  <pss_tb_qual>.SQLX_BASE COMPLETE          00260000
*                                                                       00270000
-UNLD 001030 SELECT * FROM <pss_tb_qual>.SQLX_SQLTXT                    00280000
               WHERE OBJOWNR = '<extract_creator>'                      00290000
             ;                                                          00300000
             DDNAME SYSRE002                                            00310000
-SYNC 001040 UNLOAD OF TABLE <pss_tb_qual>.SQLX_SQLTXT COMPLETE         00320000
*                                                                       00330000
-UNLD 001050 SELECT * FROM <pss_tb_qual>.SQLX_STATS                     00340000
               WHERE OBJOWNR = '<extract_creator>'                      00350000
             ;                                                          00360000
             DDNAME SYSRE003                                            00370000
-SYNC 001060 UNLOAD OF TABLE <pss_tb_qual>.SQLX_STATS COMPLETE          00380000
*                                                                       00390000

./ ADD NAME=PTDB
  SELECT * FROM ALOGSTR.ACCUM_STRATEGY                   ;

./ ADD NAME=QUERIES
SYS3.AMIOPS.V202204.ADOMSAMP(QUERIES)                                   00010000
--                                                                      00020000
--SQL EXECUTIONS WHOSE ELAPSED TIME EXCEEDED 1 SECOND, AND WHO EXECUTED 00030000
--AT LEAST 100 TIMES IN THE CURRENT MONTH, AND WHOSE ELAPSED TIME WAS   00040000
--GREATER THAN TWICE THE MONTHLY AVERAGE FOR THAT STATEMENT.            00050000
--                                                                      00060000
  SELECT X.EVNTTIME AS  TIME,                                           00070000
         X.PROGRAM, X.QUERYNO, X.CALLTYPE AS TYPE,                      00080000
         X.ELAPTIME AS ELAPSED,                                         00090000
         Y.ELAPTIME/Y.CALLS AS MONTHLY_AVERAGE,                         00100000
         Y.CALLS AS MONTHLY_CALL_COUNT,                                 00110000
         X.TEXTHASH                                                     00120000
    FROM BMCSFTWR.STMT_EXCEPTIONS X,                                    00130000
         BMCSFTWR.MONTHLY_STMT_STATISTICS Y                             00140000
    WHERE X.EXCPREAS = 'ELAPSED TM'                                     00150000
         AND X.PROGRAM = Y.PROGRAM                                      00160000
         AND X.QUERYNO = Y.QUERYNO                                      00170000
         AND X.TEXTHASH = Y.TEXTHASH                                    00180000
         AND X.CALLTYPE = Y.STMTTYPE                                    00190000
         AND X.QWHSSSID = Y.QWHSSSID                                    00200000
         AND X.SECTNO = Y.SECTNO                                        00210000
         AND X.AUTHID = Y.AUTHID                                        00220000
         AND X.CORRID = Y.CORRID                                        00230000
         AND X.RLOK = Y.RLOC                                            00240000
         AND Y.CALLS > 100                                              00250000
         AND X.ELAPTIME > (Y.ELAPTIME/Y.CALLS) * 2                      00260000
         AND X.ELAPTIME > 1                                             00270000
         AND MONTH(Y.INTVTIME) = MONTH(X.EVNTTIME)                      00280000
            ORDER BY X.EVNTTIME ASC;                                    00290000
                                                                        00300000
--                                                                      00310000
-- SELECT & DISPLAY SQL ERRORS AND COUNT OF UNIQUE SQLCODE/PROGRAM/PLAN 00320000
-- OCCURRENCES, THEN DISPLAY WITH PROGRAM/PLAN AND MIN/MAX EVENT TIMES  00330000
-- IN ASCENDING ORDER.                                                  00340000
--                                                                      00350000
-- OPTIONAL LAST LINE FOR DIFFERENT SORT:                               00360000
--     ORDER BY SQLCODE ASC, COUNT(SQLCODE) DESC                        00370000
--                                                                      00380000
--                                                                      00390000
  SELECT SQLCODE, COUNT(SQLCODE) AS OCCURRENCES, PROGRAM, PLAN,         00400000
     MIN(EVNTTIME) AS EARLIEST_OCCURENCE,                               00410000
            MAX(EVNTTIME) AS LATEST_OCCURENCE                           00420000
            FROM BMCSFTWR.STMT_ERRORS                                   00430000
            GROUP BY SQLCODE, PLAN, PROGRAM                             00440000
            ORDER BY SQLCODE ASC, PROGRAM, PLAN;                        00450000
--                                                                      00460000
-- IDENTIFY STATEMENTS WITH RID LIST FAILURES                           00470000
--                                                                      00480000
  SELECT A.LDB2NAME,A.PLAN, A.PROGRAM,                                  00490000
        A.QXNSMIAP AS RID_FAIL_NO_STRG,                                 00500000
        A.QXMRMIAP AS RID_FAIL_MAX_USED,                                00510000
        B.SQLTEXT£ ,B.SQLTEXT,                                          00520000
        B.SGCOUNT,A.TEXTHASH                                            00530000
    FROM BMCSFTWR.DAILY_STMT_STATISTICS A,                              00540000
         BMCSFTWR.STMT_TEXT B                                           00550000
      WHERE A.TEXTHASH = B.TEXTHASH  AND                                00560000
            A.QXMRMIAP > 0 OR                                           00570000
            A.QXNSMIAP > 0;                                             00580000
--                                                                      00590000
--  TO FIND WHICH STATEMENTS FAILED TO GET THE DEGREE OF                00600000
--  PARALLELISM PLANNED                                                 00610000
--                                                                      00620000
    SELECT A.LDB2NAME,A.PLAN, A.PROGRAM,                                00630000
          QXREDGRP AS NO_BUFFER, QXDEGBUF AS SEQ_NO_BUFR,               00640000
          QXDEGCUR AS UPD_CRSR,   QXDEGESA AS ESA_SORT,                 00650000
          B.SQLTEXT£ ,B.SQLTEXT, A.QXREDGRP,                            00660000
          A.TEXTHASH                                                    00670000
      FROM BMCSFTWR.DAILY_STMT_STATISTICS A JOIN                        00680000
           BMCSFTWR.STMT_TEXT          B                                00690000
        ON  A.TEXTHASH = B.TEXTHASH                                     00700000
         WHERE QXREDGRP > 0  OR                                         00710000
               QXDEGCUR > 0  OR                                         00720000
               QXDEGESA > 0  OR                                         00730000
               QXDEGBUF > 0                                             00740000
       ORDER BY A.INTVTIME,SMFID,QWHSSSID,                              00750000
                QWHSRN, QWHADSGN,                                       00760000
                QWHAMEMN,PLAN,PROGRAM,SECTNO,QUERYNO,                   00770000
                COLLID,CONTOKEN,RLOC,STMTTYNO,TRUETYPE,                 00780000
                CONNID,AUTHID,CORRID,                                   00790000
                CLNTAP,CLNTUS,CLNTWS,                                   00800000
                B.TEXTHASH, B.SQLTEXT£;                                 00810000
--                                                                      00820000
--                                                                      00830000
--  COMPARE THE HIGH, LOW AND AVERAGE RESOURCE CONSUMPTION              00840000
--   FOR A PROGRAM THAT RUNS ON MULTIPLE DB2'S BY MONTH                 00850000
--                                                                      00860000
     SELECT INTVTIME, LDB2NAME, PROGRAM,                                00870000
            HICPU, LOCPU, CPUTIME/CALLS AS  AVG_CPU,                    00880000
            HIELAPSD, LOELAPSD, ELAPTIME/CALLS AS  AVG_ELAPTIME,        00890000
            HIGETPAG, LOGETPAG, GETPAGES/CALLS AS AVG_GETPAGES          00900000
      FROM BMCSFTWR.MONTHLY_WKLD_STATISTICS                             00910000
        WHERE PROGRAM = 'MYPROG'                                        00920000
         ORDER BY INTVTIME;                                             00930000
--                                                                      00940000
--  TO FIND OUT WHICH USERS AND STATEMENTS ACCESSED A                   00950000
--  PARTICULAR OBJECT AND THE SQL TEXT USED                             00960000
--                                                                      00970000
    SELECT A.INTVTIME, A.AUTHID, A.PROGRAM, A.CREATOR, A.OBNAME,        00980000
          B.SQLTEXT£ ,B.SQLTEXT, B.SGCOUNT                              00990000
      FROM BMCSFTWR.STMT_STATISTICS_OB A JOIN                           01000000
           BMCSFTWR.STMT_TEXT          B                                01010000
        ON  A.TEXTHASH = B.TEXTHASH                                     01020000
          WHERE A.OBNAME = 'MYTABLE'                                    01030000
       ORDER BY INTVTIME,SMFID,QWHSSSID,                                01040000
                QWHSRN,QWHADSGN,QWHAMEMN,                               01050000
                PLAN,PROGRAM,SECTNO,QUERYNO,                            01060000
                COLLID,CONTOKEN,RLOK,                                   01070000
                CALLTYPE,TRUETYPE,                                      01080000
                CONNID,AUTHID,CORRID,                                   01090000
                DBNAME, PSNAME,PARTITN,OBID,                            01100000
                B.TEXTHASH, B.SQLTEXT£;                                 01110000
--                                                                      01120000
--                                                                      01130000
--  IDENTIFY OBJECTS RECOMMENDED FOR REORG                              01140000
--                                                                      01150000
  SELECT LDB2NAME, DBNAME, PSNAME, PART, TYPE                           01160000
   FROM BMCSFTWR.REORG_CANDIDATE_TABLE                                  01170000
     WHERE REORG_CANDIDATE = 'Y';                                       01180000
--                                                                      01190000
--  IDENTIFY OBJECTS WITH DEGRADED PERFORMANCE                          01200000
--                                                                      01210000
  SELECT INTVTIME, DBNAME, OBNAME, PARTITN,                             01220000
    CASE WHEN SYNCIO=0 THEN GETPAGES                                    01230000
         ELSE GETPAGES/SYNCIO                                           01240000
         END AS "GETPAGES PER SYNCIO"                                   01250000
    FROM BMCSFTWR.DAILY_OBJ_STATISTICS                                  01260000
    ORDER BY DBNAME, OBNAME, PARTITN, INTVTIME;                         01270000
--                                                                      01280000
--  COMPARE DAILY ACCOUNTING METRICS TO BASELINE FOR A                  01290000
--  SPECIFIC PROGRAM                                                    01300000
--                                                                      01310000
  SELECT A.ELAPTIME/A.CALLS AS DAILY_AVG_ELAP,                          01320000
         B.ELAPTIME/B.CALLS AS BASE_AVG_ELAP,                           01330000
         A.CPUTIME/A.CALLS AS DAILY_AVG_CPU,                            01340000
         B.CPUTIME/B.CALLS AS BASE_AVG_CPU,                             01350000
         A.GETPAGES/A.CALLS AS DAILY_GETPAGES,                          01360000
         B.GETPAGES/B.CALLS AS BASE_AVG_GETPAGES,                       01370000
         A.PLAN,                                                        01380000
         A.PROGRAM,                                                     01390000
         A.LDB2NAME                                                     01400000
   FROM (BMCSFTWR.DAILY_STMT_STATISTICS A JOIN                          01410000
         BMCSFTWR.BASELINE_STMT_STATISTICS B                            01420000
     ON  A.TEXTHASH = B.TEXTHASH AND                                    01430000
        A.LDB2NAME = B.LDB2NAME  AND                                    01440000
        A.PLAN     = B.PLAN      AND                                    01450000
        A.PROGRAM  = B.PROGRAM);                                        01460000
         WHERE A.PROGRAM = 'MYPROGRAM';                                 01470000
--                                                                      01480000
--  FIND SQLTEXT WHEN TEXTHASH VALUE IS KNOWN                           01490000
--                                                                      01500000
  SELECT SQLTEXT                                                        01510000
     FROM BMCSFTWR.STMT_TEXT                                            01520000
     WHERE TEXTHASH='9C7B3D8A6D41A563DFF7'                              01530000
     ORDER BY SQLTEXT£;                                                 01540000
                                                                        01550000
--                                                                      01560000

./ ADD NAME=QUERIES1
SYS3.AMIOPS.V202204.DOMSAMP(QUERIES)                                    00010000
--                                                                      00020000
--SQL EXECUTIONS WHOSE ELAPSED TIME EXCEEDED 1 SECOND, AND WHO EXECUTED 00030000
--AT LEAST 100 TIMES IN THE CURRENT MONTH, AND WHOSE ELAPSED TIME WAS   00040000
--GREATER THAN TWICE THE MONTHLY AVERAGE FOR THAT STATEMENT.            00050000
--                                                                      00060000
  SELECT X.EVNTTIME AS  TIME,                                           00070000
         X.PROGRAM, X.QUERYNO, X.CALLTYPE AS TYPE,                      00080000
         X.ELAPTIME AS ELAPSED,                                         00090000
         Y.ELAPTIME/Y.CALLS AS MONTHLY_AVERAGE,                         00100000
         Y.CALLS AS MONTHLY_CALL_COUNT,                                 00110000
         X.TEXTHASH                                                     00120000
    FROM BMCSFTWR.STMT_EXCEPTIONS X,                                    00130000
         BMCSFTWR.MONTHLY_STMT_STATISTICS Y                             00140000
    WHERE X.EXCPREAS = 'ELAPSED TM'                                     00150000
         AND X.PROGRAM = Y.PROGRAM                                      00160000
         AND X.QUERYNO = Y.QUERYNO                                      00170000
         AND X.TEXTHASH = Y.TEXTHASH                                    00180000
         AND X.CALLTYPE = Y.STMTTYPE                                    00190000
         AND X.QWHSSSID = Y.QWHSSSID                                    00200000
         AND X.SECTNO = Y.SECTNO                                        00210000
         AND X.AUTHID = Y.AUTHID                                        00220000
         AND X.CORRID = Y.CORRID                                        00230000
         AND X.RLOK = Y.RLOC                                            00240000
         AND Y.CALLS > 100                                              00250000
         AND X.ELAPTIME > (Y.ELAPTIME/Y.CALLS) * 2                      00260000
         AND X.ELAPTIME > 1                                             00270000
         AND MONTH(Y.INTVTIME) = MONTH(X.EVNTTIME)                      00280000
            ORDER BY X.EVNTTIME ASC;                                    00290000
                                                                        00300000
--                                                                      00310000
-- SELECT & DISPLAY SQL ERRORS AND COUNT OF UNIQUE SQLCODE/PROGRAM/PLAN 00320000
-- OCCURRENCES, THEN DISPLAY WITH PROGRAM/PLAN AND MIN/MAX EVENT TIMES  00330000
-- IN ASCENDING ORDER.                                                  00340000
--                                                                      00350000
-- OPTIONAL LAST LINE FOR DIFFERENT SORT:                               00360000
--     ORDER BY SQLCODE ASC, COUNT(SQLCODE) DESC                        00370000
--                                                                      00380000
--                                                                      00390000
  SELECT SQLCODE, COUNT(SQLCODE) AS OCCURRENCES, PROGRAM, PLAN,         00400000
     MIN(EVNTTIME) AS EARLIEST_OCCURENCE,                               00410000
            MAX(EVNTTIME) AS LATEST_OCCURENCE                           00420000
            FROM BMCSFTWR.STMT_ERRORS                                   00430000
            GROUP BY SQLCODE, PLAN, PROGRAM                             00440000
            ORDER BY SQLCODE ASC, PROGRAM, PLAN;                        00450000
--                                                                      00460000
-- IDENTIFY STATEMENTS WITH RID LIST FAILURES                           00470000
--                                                                      00480000
  SELECT A.LDB2NAME,A.PLAN, A.PROGRAM,                                  00490000
        A.QXNSMIAP AS RID_FAIL_NO_STRG,                                 00500000
        A.QXMRMIAP AS RID_FAIL_MAX_USED,                                00510000
        B.SQLTEXT£ ,B.SQLTEXT,                                          00520000
        B.SGCOUNT,A.TEXTHASH                                            00530000
    FROM BMCSFTWR.DAILY_STMT_STATISTICS A,                              00540000
         BMCSFTWR.STMT_TEXT B                                           00550000
      WHERE A.TEXTHASH = B.TEXTHASH  AND                                00560000
            A.QXMRMIAP > 0 OR                                           00570000
            A.QXNSMIAP > 0;                                             00580000
--                                                                      00590000
--  TO FIND WHICH STATEMENTS FAILED TO GET THE DEGREE OF                00600000
--  PARALLELISM PLANNED                                                 00610000
--                                                                      00620000
    SELECT A.LDB2NAME,A.PLAN, A.PROGRAM,                                00630000
          QXREDGRP AS NO_BUFFER, QXDEGBUF AS SEQ_NO_BUFR,               00640000
          QXDEGCUR AS UPD_CRSR,   QXDEGESA AS ESA_SORT,                 00650000
          B.SQLTEXT£ ,B.SQLTEXT, A.QXREDGRP,                            00660000
          A.TEXTHASH                                                    00670000
      FROM BMCSFTWR.DAILY_STMT_STATISTICS A JOIN                        00680000
           BMCSFTWR.STMT_TEXT          B                                00690000
        ON  A.TEXTHASH = B.TEXTHASH                                     00700000
         WHERE QXREDGRP > 0  OR                                         00710000
               QXDEGCUR > 0  OR                                         00720000
               QXDEGESA > 0  OR                                         00730000
               QXDEGBUF > 0                                             00740000
       ORDER BY A.INTVTIME,SMFID,QWHSSSID,                              00750000
                QWHSRN, QWHADSGN,                                       00760000
                QWHAMEMN,PLAN,PROGRAM,SECTNO,QUERYNO,                   00770000
                COLLID,CONTOKEN,RLOC,STMTTYNO,TRUETYPE,                 00780000
                CONNID,AUTHID,CORRID,                                   00790000
                CLNTAP,CLNTUS,CLNTWS,                                   00800000
                B.TEXTHASH, B.SQLTEXT£;                                 00810000
--                                                                      00820000
--                                                                      00830000
--  COMPARE THE HIGH, LOW AND AVERAGE RESOURCE CONSUMPTION              00840000
--   FOR A PROGRAM THAT RUNS ON MULTIPLE DB2'S BY MONTH                 00850000
--                                                                      00860000
     SELECT INTVTIME, LDB2NAME, PROGRAM,                                00870000
            HICPU, LOCPU, CPUTIME/CALLS AS  AVG_CPU,                    00880000
            HIELAPSD, LOELAPSD, ELAPTIME/CALLS AS  AVG_ELAPTIME,        00890000
            HIGETPAG, LOGETPAG, GETPAGES/CALLS AS AVG_GETPAGES          00900000
      FROM BMCSFTWR.MONTHLY_WKLD_STATISTICS                             00910000
        WHERE PROGRAM = 'MYPROG'                                        00920000
         ORDER BY INTVTIME;                                             00930000
--                                                                      00940000
--  TO FIND OUT WHICH USERS AND STATEMENTS ACCESSED A                   00950000
--  PARTICULAR OBJECT AND THE SQL TEXT USED                             00960000
--                                                                      00970000
    SELECT A.INTVTIME, A.AUTHID, A.PROGRAM, A.CREATOR, A.OBNAME,        00980000
          B.SQLTEXT£ ,B.SQLTEXT, B.SGCOUNT                              00990000
      FROM BMCSFTWR.STMT_STATISTICS_OB A JOIN                           01000000
           BMCSFTWR.STMT_TEXT          B                                01010000
        ON  A.TEXTHASH = B.TEXTHASH                                     01020000
          WHERE A.OBNAME = 'MYTABLE'                                    01030000
       ORDER BY INTVTIME,SMFID,QWHSSSID,                                01040000
                QWHSRN,QWHADSGN,QWHAMEMN,                               01050000
                PLAN,PROGRAM,SECTNO,QUERYNO,                            01060000
                COLLID,CONTOKEN,RLOK,                                   01070000
                CALLTYPE,TRUETYPE,                                      01080000
                CONNID,AUTHID,CORRID,                                   01090000
                DBNAME, PSNAME,PARTITN,OBID,                            01100000
                B.TEXTHASH, B.SQLTEXT£;                                 01110000
--                                                                      01120000
--                                                                      01130000
--  IDENTIFY OBJECTS RECOMMENDED FOR REORG                              01140000
--                                                                      01150000
  SELECT LDB2NAME, DBNAME, PSNAME, PART, TYPE                           01160000
   FROM BMCSFTWR.REORG_CANDIDATE_TABLE                                  01170000
     WHERE REORG_CANDIDATE = 'Y';                                       01180000
--                                                                      01190000
--  IDENTIFY OBJECTS WITH DEGRADED PERFORMANCE                          01200000
--                                                                      01210000
  SELECT INTVTIME, DBNAME, OBNAME, PARTITN,                             01220000
    CASE WHEN SYNCIO=0 THEN GETPAGES                                    01230000
         ELSE GETPAGES/SYNCIO                                           01240000
         END AS "GETPAGES PER SYNCIO"                                   01250000
    FROM BMCSFTWR.DAILY_OBJ_STATISTICS                                  01260000
    ORDER BY DBNAME, OBNAME, PARTITN, INTVTIME;                         01270000
--                                                                      01280000
--  COMPARE DAILY ACCOUNTING METRICS TO BASELINE FOR A                  01290000
--  SPECIFIC PROGRAM                                                    01300000
--                                                                      01310000
  SELECT A.ELAPTIME/A.CALLS AS DAILY_AVG_ELAP,                          01320000
         B.ELAPTIME/B.CALLS AS BASE_AVG_ELAP,                           01330000
         A.CPUTIME/A.CALLS AS DAILY_AVG_CPU,                            01340000
         B.CPUTIME/B.CALLS AS BASE_AVG_CPU,                             01350000
         A.GETPAGES/A.CALLS AS DAILY_GETPAGES,                          01360000
         B.GETPAGES/B.CALLS AS BASE_AVG_GETPAGES,                       01370000
         A.PLAN,                                                        01380000
         A.PROGRAM,                                                     01390000
         A.LDB2NAME                                                     01400000
   FROM (BMCSFTWR.DAILY_STMT_STATISTICS A JOIN                          01410000
         BMCSFTWR.BASELINE_STMT_STATISTICS B                            01420000
     ON  A.TEXTHASH = B.TEXTHASH AND                                    01430000
        A.LDB2NAME = B.LDB2NAME  AND                                    01440000
        A.PLAN     = B.PLAN      AND                                    01450000
        A.PROGRAM  = B.PROGRAM);                                        01460000
         WHERE A.PROGRAM = 'MYPROGRAM';                                 01470000
--                                                                      01480000
--  FIND SQLTEXT WHEN TEXTHASH VALUE IS KNOWN                           01490000
--                                                                      01500000
  SELECT SQLTEXT                                                        01510000
     FROM BMCSFTWR.STMT_TEXT                                            01520000
     WHERE TEXTHASH='9C7B3D8A6D41A563DFF7'                              01530000
     ORDER BY SQLTEXT£;                                                 01540000
                                                                        01550000
--                                                                      01560000

