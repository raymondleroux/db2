-----------------------------------------------------------------------
-- Create the DB2 optimization routine
-----------------------------------------------------------------------
 CREATE PROCEDURE SYSPROC.DSNAHVPM
   ( IN    STMTNOI             INTEGER
   , IN    SECTNOI             INTEGER
   , IN    NAME                VARCHAR(128)
   , IN    PLCREATOR           VARCHAR(128)
   , IN    PLNAME              VARCHAR(128)
   , IN    COLLID              VARCHAR(128)
   , IN    VERSION             VARCHAR(128)
   , IN    DBRM                CLOB(2M)
   , OUT   ERRORCODE           INTEGER
   , OUT   ERRORMSG            VARCHAR(960)
   )
   PARAMETER CCSID EBCDIC
   RESULT SETS 1
   LANGUAGE C
   EXTERNAL NAME DSNAHVPM
   MODIFIES SQL DATA
   COLLID DSNAHVPM
   PARAMETER STYLE GENERAL
   STAY RESIDENT NO
   WLM ENVIRONMENT DSNWLM_GENERAL
   SECURITY DB2
   PROGRAM TYPE MAIN ;

-----------------------------------------------------------------------
-- Create the DB2 Runtime statistics processing routine
-----------------------------------------------------------------------
 CREATE PROCEDURE SYSPROC.DSNACCOX
   ( IN    QUERYTYPE           VARCHAR(40)
   , IN    OBJECTTYPE          VARCHAR(3)
   , IN    ICTYPE              VARCHAR(1)
   , IN    CATLGSCHEMA         VARCHAR(128)
   , IN    LOCALSCHEMA         VARCHAR(128)
   , IN    CHKLVL              INTEGER
   , IN    CRITERIA            VARCHAR(4096)
   , IN    SPECIALPARM         VARCHAR(80)
   , IN    CRUPDATEDPAGESPCT   DOUBLE
   , IN    CRUPDATEDPAGESABS   INTEGER
   , IN    CRCHANGESPCT        DOUBLE
   , IN    CRDAYSNCLASTCOPY    INTEGER
   , IN    ICRUPDATEDPAGESPCT  DOUBLE
   , IN    ICRUPDATEDPAGESABS  INTEGER
   , IN    ICRCHANGESPCT       DOUBLE
   , IN    CRINDEXSIZE         INTEGER
   , IN    RRTINSERTSPCT       DOUBLE
   , IN    RRTINSERTSABS       INTEGER
   , IN    RRTDELETESPCT       DOUBLE
   , IN    RRTDELETESABS       INTEGER
   , IN    RRTUNCLUSTINSPCT    DOUBLE
   , IN    RRTDISORGLOBPCT     DOUBLE
   , IN    RRTDATASPACERAT     DOUBLE
   , IN    RRTMASSDELLIMIT     INTEGER
   , IN    RRTINDREFLIMIT      DOUBLE
   , IN    RRIINSERTSPCT       DOUBLE
   , IN    RRIINSERTSABS       INTEGER
   , IN    RRIDELETESPCT       DOUBLE
   , IN    RRIDELETESABS       INTEGER
   , IN    RRIAPPENDINSERTPCT  DOUBLE
   , IN    RRIPSEUDODELETEPCT  DOUBLE
   , IN    RRIMASSDELLIMIT     INTEGER
   , IN    RRILEAFLIMIT        DOUBLE
   , IN    RRINUMLEVELSLIMIT   INTEGER
   , IN    SRTINSDELUPDPCT     DOUBLE
   , IN    SRTINSDELUPDABS     INTEGER
   , IN    SRTMASSDELLIMIT     INTEGER
   , IN    SRIINSDELPCT        DOUBLE
   , IN    SRIINSDELABS        INTEGER
   , IN    SRIMASSDELLIMIT     INTEGER
   , IN    EXTENTLIMIT         INTEGER
   , OUT   LASTSTATEMENT       VARCHAR(8012)
   , OUT   RETURNCODE          INTEGER
   , OUT   ERRORMSG            VARCHAR(1331)
   , OUT   IFCARETCODE         INTEGER
   , OUT   IFCARESCODE         INTEGER
   , OUT   XSBYTES             INTEGER
   )
   PARAMETER CCSID EBCDIC
   PROGRAM TYPE MAIN
   MODIFIES SQL DATA
   EXTERNAL NAME DSNACCOX
   COLLID DSNACCOX
   LANGUAGE C
   RUN OPTIONS 'TRAP(OFF),STACK(,,ANY,)'
   ASUTIME NO LIMIT
   STAY RESIDENT NO
   WLM ENVIRONMENT DSNWLM_GENERAL
   COMMIT ON RETURN NO
   PARAMETER STYLE GENERAL WITH NULLS
   RESULT SETS 2 ;

-----------------------------------------------------------------------
-- Create the CGTTs for the Java and SQL procedure processing routines
-----------------------------------------------------------------------
 CREATE GLOBAL TEMPORARY TABLE SYSIBM.JSPOUT
   ( STEP                      VARCHAR(16)
   , FILE                      VARCHAR(8)
   , SEQN                      INTEGER
   , LINE                      VARCHAR(255)
   )
   CCSID  EBCDIC ;

 CREATE GLOBAL TEMPORARY TABLE SYSIBM.SYSPSMOUT
   ( STEP                      VARCHAR(16)
   , FILE                      VARCHAR(8)
   , SEQN                      INTEGER
   , LINE                      VARCHAR(255)
   )
   CCSID EBCDIC ;

-----------------------------------------------------------------------
-- Create the DB2 Java and SQL procedure processing routines
-----------------------------------------------------------------------
 CREATE PROCEDURE SQLJ.ALTER_JAVA_PATH
   ( IN    JARNAME             VARCHAR(257)
   , IN    PATH                VARCHAR(2048)
   )
   PARAMETER CCSID EBCDIC
   EXTERNAL NAME DSNX9AJP
   LANGUAGE C
   WLM ENVIRONMENT DSNWLM_GENERAL
   RUN OPTIONS 'POSIX(ON)'
   PROGRAM TYPE SUB
   MODIFIES SQL DATA
   ASUTIME NO LIMIT
   STAY RESIDENT YES
   COMMIT ON RETURN NO
   PARAMETER STYLE DB2SQL
   COLLID DSNJAR
   SECURITY USER ;

 CREATE PROCEDURE SQLJ.DB2_INSTALL_JAR
   ( IN    AJAR                BLOB AS LOCATOR
    ,IN    JARNAME             VARCHAR(257)
    ,IN    DEPLOY              INTEGER
   )
   PARAMETER CCSID EBCDIC
   EXTERNAL NAME DSNX9DIJ
   LANGUAGE C
   WLM ENVIRONMENT DSNWLM_GENERAL
   RUN OPTIONS 'POSIX(ON)'
   PROGRAM TYPE SUB
   MODIFIES SQL DATA
   ASUTIME NO LIMIT
   STAY RESIDENT YES
   COMMIT ON RETURN NO
   PARAMETER STYLE DB2SQL
   COLLID DSNJAR
   SECURITY USER ;

 CREATE PROCEDURE SQLJ.DB2_REMOVE_JAR
   ( IN    JARNAME             VARCHAR(257)
   , IN    UNDEPLOY            INTEGER
   )
   PARAMETER CCSID EBCDIC
   EXTERNAL NAME DSNX9DRJ
   LANGUAGE C
   WLM ENVIRONMENT DSNWLM_GENERAL
   RUN OPTIONS 'POSIX(ON)'
   PROGRAM TYPE SUB
   MODIFIES SQL DATA
   ASUTIME NO LIMIT
   STAY RESIDENT YES
   COMMIT ON RETURN NO
   PARAMETER STYLE DB2SQL
   COLLID DSNJAR
   SECURITY USER ;

 CREATE PROCEDURE SQLJ.DB2_REPLACE_JAR
   ( IN    AJAR                BLOB AS LOCATOR
    ,IN    JARNAME             VARCHAR(257)
   )
   PARAMETER CCSID EBCDIC
   EXTERNAL NAME DSNX9DPJ
   LANGUAGE C
   WLM ENVIRONMENT DSNWLM_GENERAL
   RUN OPTIONS 'POSIX(ON)'
   PROGRAM TYPE SUB
   MODIFIES SQL DATA
   ASUTIME NO LIMIT
   STAY RESIDENT YES
   COMMIT ON RETURN NO
   PARAMETER STYLE DB2SQL
   COLLID DSNJAR
   SECURITY USER ;

