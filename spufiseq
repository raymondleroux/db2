./ ADD NAME=DSNTESE
                                                                        00000100
        -- USE THE SAMPLES AUTHORIZATION ID                             00000200
           SET CURRENT SQLID = 'DSN8§§0' ;                              00000300
                                                                        00000400
                                                                        00000500
           EXPLAIN PLAN SET QUERYNO = 9 FOR                             00000600
           SELECT DISTINCT                                              00000700
             DEPTNO, DEPTNAME, MGRNO,                                   00000800
             FIRSTNME, MIDINIT, LASTNAME                                00000900
           FROM VDEPMG1                                                 00001000
           WHERE DEPTNO     LIKE 'D_1'                                  00001100
             AND DEPTNAME   LIKE '%'                                    00001200
             AND MGRNO      LIKE '%'                                    00001300
             AND LASTNAME LIKE '%'                                      00001400
             AND '%'         =   '%'                                    00001500
             AND '%'         =   '%'                                    00001600
             AND DEPTNO      >   'D01';                                 00001700
                                                                        00001800
           SELECT * FROM PLAN_TABLE WHERE QUERYNO = 9 ;                 00001900
                                                                        00002000
        -- RETURN TO PRIMARY AUTHORIZATION ID                           00002100
           SET CURRENT SQLID = USER ;                                   00002200

./ ADD NAME=DSNTESH
