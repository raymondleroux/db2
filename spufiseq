   , IN    SCHEMAPROPERTIES    BLOB(5M)
   , IN    ISUSEDFORDECOMPOSITION INTEGER
   )
   PARAMETER CCSID  UNICODE
   EXTERNAL NAME
   'COM.ibm.db2os390.v10.dsnnvxcv.xsr_complete(java.lang.String,
   java.lang.String,java.sql.Blob,java.lang.Integer)'
   COLLID SYSXSR
   LANGUAGE JAVA
   WLM ENVIRONMENT DSNWLM_JAVA
   FENCED DETERMINISTIC
   STAY RESIDENT YES
   PROGRAM TYPE SUB
   PARAMETER STYLE JAVA ;

