./ ADD NAME=ASUINIT2
   CREATE TABLE AMIDB2.EVENTS
      ( WORKID      CHAR(20)    NOT NULL  WITH DEFAULT,
        TASKID      CHAR(8)     NOT NULL  WITH DEFAULT,
        FUNCTION    CHAR(8)     NOT NULL  WITH DEFAULT,
        QUALIFIER   VARCHAR(128) NOT NULL WITH DEFAULT,
        OBJECT      VARCHAR(128) NOT NULL  WITH DEFAULT,
        OBJTYPE     CHAR(10)    NOT NULL  WITH DEFAULT,
        RETCODE     SMALLINT    NOT NULL  WITH DEFAULT,
        TIMESTMP    TIMESTAMP   NOT NULL  WITH DEFAULT,
        RUNFLAG     CHAR(1)     NOT NULL  WITH DEFAULT,
        ELAPTIME    SMALLINT    NOT NULL  WITH DEFAULT,
        CPUTIME     SMALLINT    NOT NULL  WITH DEFAULT,
        DSN         VARCHAR(56) NOT NULL  WITH DEFAULT,
        PARMS       VARCHAR(512) NOT NULL WITH DEFAULT,
        VERSION  CHAR(8) NOT NULL WITH DEFAULT '13.01.00' FOR SBCS DATA
      ) IN &DDMDBNM..BMCRSEV      WITH RESTRICT ON DROP
*
   COMMENT ON TABLE AMIDB2.EVENTS IS
       'STATISTICS TABLE FOR DB2 DASD MANAGER EVENTS'
*
   CREATE TABLE AMIDB2.EXCEPTIONS2
      ( QUALIFIER          VARCHAR(128) NOT NULL,
        OBJECT             VARCHAR(128)    NOT NULL,
        PARTITION          SMALLINT        NOT NULL,
        OBJTYPE            CHAR(002)       NOT NULL,
        EXCEPTION          CHAR(008)       NOT NULL,
        CURRENT_VALUEF     FLOAT(004)      NOT NULL WITH DEFAULT,
        COMPARE_VALUEF     FLOAT(004)      NOT NULL WITH DEFAULT,
        TRIGGER_VALUE      INTEGER         NOT NULL WITH DEFAULT,
        TIMESTMP           TIMESTAMP       NOT NULL WITH DEFAULT,
        ACTION_TAKEN       VARCHAR(060)    NOT NULL WITH DEFAULT,
        JCLDSN             VARCHAR(056)    NOT NULL WITH DEFAULT,
        ACTIVE             CHAR(1)         NOT NULL WITH DEFAULT 'Y',
        LAST_UPDATE        TIMESTAMP       NOT NULL WITH DEFAULT,
        LAST_UPDATER       CHAR(8)         NOT NULL WITH DEFAULT,
        PRIORITY           SMALLINT        NOT NULL WITH DEFAULT,
        CORRECTIVE_ACTION  CHAR(20)        NOT NULL WITH DEFAULT,
        REGISTRATION_ID    INTEGER                              ,
        BMCTRIG_ID         CHAR(20)        NOT NULL WITH DEFAULT,
        VERSION  CHAR(8) NOT NULL WITH DEFAULT '13.01.00' FOR SBCS DATA,
        PRIMARY KEY        (QUALIFIER
                           ,OBJECT
                           ,PARTITION
                           ,OBJTYPE
                           ,EXCEPTION
                           ,TIMESTMP)
      ) IN &DDMDBNM..BMCRSEX      WITH RESTRICT ON DROP
*
   COMMENT ON TABLE AMIDB2.EXCEPTIONS2 IS
       'EXCEPTIONS TABLE FOR DB2 DASD MANAGER'
*
   CREATE TABLE AMIDB2.WL_SYNC
      ( ALTERID             CHAR (8)      NOT NULL WITH DEFAULT,
        SYNCWKID            CHAR (20)     NOT NULL WITH DEFAULT,
