./ ADD NAME=EXCEPTIO
                                                       TABLE            00000100
--CREATING AN EXCEPTION TABLE FOR THE PROJECT ACTIVITY                  00001000
EXEC SQL                                                                00010000
CREATE TABLE EPROJACT                                                   00020000
LIKE DSN8C10.PROJACT                                                    00030000
IN DATABASE DSN8D12A                                                    00040000
ENDEXEC                                                                 00050000
EXEC SQL                                                                00060000
ALTER TABLE EPROJACT                                                    00070000
ADD RID CHAR(4)                                                         00080000
ENDEXEC                                                                 00090000
EXEC SQL                                                                00100000
ALTER TABLE EPROJACT                                                    00110000
ADD TIME TIMESTAMP NOT NULL WITH DEFAULT                                00120000
ENDEXEC                                                                 00130000

