./ ADD NAME=DSNTESA
----------------------------------------------------------------------- 00001000
--  NAME = DSNTESA                                                      00002000
--                                                                      00003000
--  DESCRIPTIVE NAME = CHECK DELIMITER AND DECIMAL POINT SETTINGS       00004000
--                                                                      00005000
--     LICENSED MATERIALS - PROPERTY OF IBM                             00006000
--     5615-DB2                                                         00007001
--     (C) COPYRIGHT 1982, 2013 IBM CORP.  ALL RIGHTS RESERVED.         00008001
--                                                                      00009000
--     STATUS = VERSION 11                                              00009100
--                                                                      00009200
--  FUNCTION = CHECK SQL STRING DELIMITER OPTION AND DECIMAL POINT      00009300
--             OPTION                                                   00009400
--                                                                      00009500
--  CHANGE ACTIVITY                                                     00009600
--    11/07/2012 Add SET CURRENT SQLID           dn1651_inst1 / dn1651  00009700
--    05/17/2013 FIX COPYRIGHT STATEMENT                 49779_077_724  00009801
----------------------------------------------------------------------- 00009900
                                                                        00010000
----------------------------------------------------------------------- 00010100
-- Set the current authorization ID to perform the work                 00010200
----------------------------------------------------------------------- 00010300
  SET CURRENT SQLID = 'SYSADM';                                         00010400
                                                                        00010500
  CREATE DATABASE TEMPDB CCSID EBCDIC;                                  00011000
  CREATE TABLESPACE TEMPSPAC IN TEMPDB CCSID EBCDIC;                    00020000
  CREATE TABLE TEMPTABLE                                                00030000
    (NAME CHAR(8),                                                      00040000
     MIDTERM SMALLINT,                                                  00050000
     FINAL   SMALLINT,                                                  00060000
     SUM     SMALLINT,                                                  00070000
     GRADE CHAR(1) )                                                    00080000
  IN TEMPDB.TEMPSPAC                                                    00090000
  CCSID EBCDIC;                                                         00100000
  INSERT INTO TEMPTABLE ( NAME, MIDTERM, FINAL )                        00110000
    VALUES ( 'JOE',  55, 68 );                                          00120000
  INSERT INTO TEMPTABLE ( NAME, MIDTERM, FINAL )                        00130000
    VALUES ( 'SUE',  83, 87 );                                          00140000
  INSERT INTO TEMPTABLE ( NAME, MIDTERM, FINAL )                        00150000
    VALUES ( 'KEN',  50, 65 );                                          00160000
  INSERT INTO TEMPTABLE ( NAME, MIDTERM, FINAL )                        00170000
    VALUES ( 'JAN',  92, 95 );                                          00180000
  INSERT INTO TEMPTABLE ( NAME, MIDTERM, FINAL )                        00190000
    VALUES ( 'LES',  67, 75 );                                          00200000
  INSERT INTO TEMPTABLE ( NAME, MIDTERM, FINAL )                        00210000
    VALUES ( 'SALLY', 75, 78 );                                         00220000
  SELECT AVG(FINAL) FROM TEMPTABLE;                                     00230000
  SELECT AVG(MIDTERM) FROM TEMPTABLE;                                   00240000
  UPDATE TEMPTABLE SET SUM = .6 * FINAL + .4 * MIDTERM;                 00250000
  SELECT AVG(SUM) , MIN(SUM), MAX(SUM) FROM TEMPTABLE;                  00260000
  UPDATE TEMPTABLE SET GRADE = 'F' WHERE SUM < 61;                      00270000
  UPDATE TEMPTABLE SET GRADE = 'D' WHERE SUM BETWEEN 61 AND 70;         00280000
  UPDATE TEMPTABLE SET GRADE = 'C' WHERE SUM BETWEEN 71 AND 80;         00290000
  UPDATE TEMPTABLE SET GRADE = 'B' WHERE SUM BETWEEN 81 AND 90;         00300000
  UPDATE TEMPTABLE SET GRADE = 'A' WHERE SUM > 90;                      00310000
  SELECT * FROM TEMPTABLE;                                              00320000
  ROLLBACK;                                                             00330000
  SELECT NAME, BINDDATE                                                 00340000
         FROM SYSIBM.SYSPLAN                                            00350000
         WHERE CREATOR = USER    ;                                      00360000
  SELECT COLLID, NAME, VERSION, BINDTIME                                00370000
         FROM SYSIBM.SYSPACKAGE                                         00380000
         WHERE OWNER = USER    ;                                        00390000
  SELECT NAME, CREATOR, BINDDATE, VALID, OPERATIVE                      00400000
         FROM SYSIBM.SYSPLAN                                            00410000
         WHERE OPERATIVE = 'N' OR VALID = 'N';                          00420000
  SELECT COLLID, NAME, VERSION, BINDTIME, VALID                         00430000
         FROM SYSIBM.SYSPACKAGE                                         00440000
         WHERE OPERATIVE = 'N' OR VALID = 'N';                          00450000
  SELECT DNAME, BTYPE, BCREATOR, BNAME                                  00460000
         FROM SYSIBM.SYSPLANDEP                                         00470000
         WHERE BCREATOR = USER                                          00480000
         ORDER BY DNAME, BTYPE, BCREATOR, BNAME;                        00490000
  SELECT DCOLLID, DNAME, BTYPE, BQUALIFIER, BNAME                       00500000
         FROM SYSIBM.SYSPACKDEP                                         00510000
         WHERE BQUALIFIER = USER                                        00520000
         ORDER BY DCOLLID, DNAME, BTYPE, BQUALIFIER, BNAME;             00530000
  SELECT NAME, TYPE                                                     00540000
         FROM SYSIBM.SYSTABLES                                          00550000
         WHERE CREATOR = USER                                           00560000
         ORDER BY NAME, TYPE;                                           00570000
  SELECT *                                                              00580000
         FROM SYSIBM.SYSTABLES                                          00590000
         WHERE NAME = 'DEPT';                                           00600000
  SELECT NAME, COLTYPE, LENGTH, SCALE, NULLS, REMARKS, COLNO            00610000
         FROM SYSIBM.SYSCOLUMNS                                         00620000
         WHERE TBNAME= 'DEPT' AND TBCREATOR = 'DSN8§§0'                 00630000
         ORDER BY COLNO;                                                00640000
  SELECT * FROM SYSIBM.SYSPLANAUTH WHERE GRANTEE = USER;                00650000
  SELECT * FROM SYSIBM.SYSPACKAUTH WHERE GRANTEE = USER;                00660000
  SELECT * FROM SYSIBM.SYSUSERAUTH WHERE GRANTEE = USER;                00670000
  SELECT * FROM SYSIBM.SYSDBAUTH   WHERE GRANTEE = USER;                00680000
  SELECT * FROM SYSIBM.SYSTABAUTH  WHERE GRANTEE = USER;                00690000
  SELECT * FROM SYSIBM.SYSCOLAUTH  WHERE GRANTEE = USER;                00700000
  SELECT * FROM SYSIBM.SYSRESAUTH  WHERE GRANTEE = USER;                00710000
  SELECT TCREATOR, TTNAME, STNAME, GRANTOR                              00720000
         FROM SYSIBM.SYSTABAUTH                                         00730000
         WHERE GRANTEE = USER;                                          00740000
  SELECT BNAME, BTYPE, GRANTOR, NAME                                    00750000
         FROM SYSIBM.SYSPLANAUTH, SYSIBM.SYSPLANDEP                     00760000
         WHERE GRANTEE = USER                                           00770000
         AND NAME = DNAME                                               00780000
         AND EXECUTEAUTH <> ' '                                         00790000
         AND ( BTYPE = 'T' OR BTYPE = 'V');                             00800000
  SELECT DCOLLID, BNAME, BTYPE, BQUALIFIER, BNAME                       00810000
         FROM SYSIBM.SYSPACKAUTH, SYSIBM.SYSPACKDEP                     00820000
         WHERE GRANTEE = USER                                           00830000
         AND LOCATION = DLOCATION                                       00831000
         AND COLLID = DCOLLID                                           00840000
         AND NAME = DNAME                                               00850000
         AND EXECUTEAUTH <> ' '                                         00860000
         AND ( BTYPE = 'T' OR BTYPE = 'V');                             00870000
  SELECT NAME, CREATOR, TYPE, DBNAME, TSNAME                            00880000
         FROM SYSIBM.SYSTABLES                                          00890000
         WHERE DBNAME IN                                                00900000
           (SELECT NAME FROM SYSIBM.SYSDBAUTH                           00910000
                   WHERE GRANTEE = USER                                 00920000
                   AND DBADMAUTH <> ' ');                               00930000

