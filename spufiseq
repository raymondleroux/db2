   ASUTIME NO LIMIT
   STAY RESIDENT NO
   PROGRAM TYPE MAIN
   SECURITY DB2
   RUN OPTIONS 'TRAP(OFF),STACK(,,ANY,),POSIX(ON) '
   COMMIT ON RETURN NO ;

