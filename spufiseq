        SYNCSEQ             CHAR (6)      NOT NULL WITH DEFAULT,
        SYNCOWNR            CHAR (8)      NOT NULL WITH DEFAULT,
        SYNCAUTH            CHAR (8)      NOT NULL WITH DEFAULT,
        SYNCDATA            CHAR (59)     NOT NULL WITH DEFAULT,
        SYNCGLID            CHAR (8)      NOT NULL WITH DEFAULT,
        SYNCSETS            CHAR (59)     NOT NULL WITH DEFAULT,
        SYNCTSTP            TIMESTAMP     NOT NULL WITH DEFAULT,
        VERSION  CHAR(8) NOT NULL WITH DEFAULT '13.01.00' FOR SBCS DATA
      ) IN &DDMDBNM..BMCDSYNC     WITH RESTRICT ON DROP

   COMMENT ON TABLE AMIDB2.WL_SYNC IS
       'EXECUTION SYNC TABLE'
*
   CREATE TABLE AMIDB2.DO_WORKIDS
      ( WORKID        CHAR (20)    NOT NULL WITH DEFAULT,
        TASKID        CHAR (8)     NOT NULL WITH DEFAULT,
