   PROGRAM TYPE MAIN
   RUN OPTIONS 'TRAP(OFF), HEAP(,,ANY),BELOW(4K,,),ALL31(ON),
                STACK(,,ANY,),POSIX(ON),XPLINK(ON)'
   COMMIT ON RETURN NO
   INHERIT SPECIAL REGISTERS
   WLM ENVIRONMENT DSNWLM_GENERAL ;

