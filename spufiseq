           SST_SNTX_NM      CHAR(40)     NOT NULL,
           SST_SEQ_NBR      SMALLINT     NOT NULL,
           SST_SNTXT        VARCHAR(80)  NOT NULL WITH DEFAULT,
           SST_MNT_UID      CHAR(8)      NOT NULL WITH DEFAULT,
           SST_MNT_TSTMP    TIMESTAMP    NOT NULL WITH DEFAULT,
      VERSION CHAR(8) NOT NULL WITH DEFAULT '13.01.00' FOR SBCS DATA,
           PRIMARY KEY        (SST_SVC_NM
                              ,SST_SNTX_NM
                              ,SST_SEQ_NBR)
      ) IN &DDMDBNM..BMCSVSNT     WITH RESTRICT ON DROP
*
   CREATE TABLE AMIDB2.THRESHOLDS
          (TH_QUAL            VARCHAR(128) NOT NULL FOR SBCS DATA,
           TH_NAME            VARCHAR(128) NOT NULL FOR SBCS DATA,
           TH_OBJ_TYP         CHAR(2)      NOT NULL,
           TH_XCPT_NM         CHAR(8)      NOT NULL,
           TH_VALUE           INTEGER      NOT NULL WITH DEFAULT,
           TH_PRTY            SMALLINT     NOT NULL WITH DEFAULT,
           TH_ENBLD_CD        CHAR(1)      NOT NULL WITH DEFAULT 'N',
           TH_REXX_NM         CHAR(8)      NOT NULL WITH DEFAULT,
           TH_MNT_UID         CHAR(8)      NOT NULL WITH DEFAULT,
           TH_MNT_TSTMP       TIMESTAMP    NOT NULL WITH DEFAULT,
      VERSION CHAR(8) NOT NULL WITH DEFAULT '13.01.00' FOR SBCS DATA,
           TH_OBJSET_OSNAME  VARCHAR(27),
           PRIMARY KEY        (TH_QUAL
                              ,TH_NAME
                              ,TH_OBJ_TYP
                              ,TH_XCPT_NM
                              ,TH_VALUE)
      ) IN &DDMDBNM..BMCTHRES     WITH RESTRICT ON DROP
*
