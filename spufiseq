   DETERMINISTIC
   SECURITY USER ;

-----------------------------------------------------------------------
-- Create the database for DB2 JSON support
-----------------------------------------------------------------------
 CREATE DATABASE DSN5JSDB
   STOGROUP SYSDEFLT
   BUFFERPOOL BP0
   CCSID UNICODE;

 CREATE TABLESPACE DSN5JSTS
   IN DSN5JSDB
   USING STOGROUP SYSDEFLT
   BUFFERPOOL BP0
   CCSID UNICODE
   LOCKSIZE ROW
   LOCKMAX SYSTEM
   CLOSE NO;

 CREATE TABLE SYSTOOLS.SYSJSON_INDEX
   ( ID                     INTEGER       NOT NULL
                                          PRIMARY KEY
                                          GENERATED
                                            BY DEFAULT
                                            AS IDENTITY
   , IDXNAME                VARCHAR(128)
   , COLLECTION             VARCHAR(255)
   , FIELD                  VARCHAR(255)
   , DATATYPE               VARCHAR(30)
   , LENGTH                 INTEGER
   , COLNAME                VARCHAR(128)
   , DIRECTION              INTEGER
   , UNIQUE                 INTEGER
   , IDXTYPE                VARCHAR(20)
   , SKIPINVALID            INTEGER
   , COLLECTION_SCHEMA      VARCHAR(128)
   , LAST_UPDATE            TIMESTAMP     NOT NULL
                                          GENERATED ALWAYS
                                          FOR EACH ROW
                                            ON UPDATE AS ROW
                                            CHANGE TIMESTAMP
   , OPTIONS                VARCHAR(2048)
   )
   IN DSN5JSDB.DSN5JSTS;

 CREATE UNIQUE INDEX SYSTOOLS.SYSJSON_INDEX_IX1
    ON SYSTOOLS.SYSJSON_INDEX
    ( ID
    )
    USING STOGROUP SYSDEFLT;

-----------------------------------------------------------------------
-- Create the DB2 JSON processing routines
-----------------------------------------------------------------------
 CREATE FUNCTION SYSTOOLS.BSON2JSON
   ( INBSON                 BLOB(16M)
   )
   RETURNS                  CLOB(16M)
   SPECIFIC BSON2JSON
   LANGUAGE C
   PARAMETER STYLE SQL
   PARAMETER CCSID UNICODE
   WLM ENVIRONMENT DSNWLM_GENERAL
   RUN OPTIONS 'XPLINK(ON)'
   DBINFO
   PROGRAM TYPE SUB
   DISALLOW PARALLEL
   NO SQL
   DETERMINISTIC
   RETURNS NULL ON NULL INPUT
   NO EXTERNAL ACTION
   EXTERNAL NAME 'DSN5JSBJ';

 CREATE FUNCTION SYSTOOLS.JSON_BINARY
   ( INJSON                 BLOB(16M)
   , INELEM                 VARCHAR(2048)
   , MAXLENGTH              INTEGER
   )
   RETURNS                  VARCHAR(2050) FOR BIT DATA
   SPECIFIC SYSTOOLS.JSON_BINARY
   LANGUAGE C
   PARAMETER STYLE SQL
   PARAMETER CCSID UNICODE
   WLM ENVIRONMENT DSNWLM_GENERAL
   RUN OPTIONS 'XPLINK(ON)'
   DBINFO
   PROGRAM TYPE SUB
   DISALLOW PARALLEL
   NO SQL
   DETERMINISTIC
   RETURNS NULL ON NULL INPUT
   NO EXTERNAL ACTION
   EXTERNAL NAME DSN5JSBI;

 CREATE FUNCTION SYSTOOLS.JSON_BINARY2
   ( INJSON                 BLOB(16M)
   , INELEM                 VARCHAR(2048)
   , MAXLENGTH              INTEGER
   )
   RETURNS                  VARCHAR(2050) FOR BIT DATA
   SPECIFIC SYSTOOLS.JSON_BINARY2
   LANGUAGE C
   PARAMETER STYLE SQL
   PARAMETER CCSID UNICODE
   WLM ENVIRONMENT DSNWLM_GENERAL
   RUN OPTIONS 'XPLINK(ON)'
   DBINFO
   PROGRAM TYPE SUB
   DISALLOW PARALLEL
   NO SQL
   DETERMINISTIC
   RETURNS NULL ON NULL INPUT
   NO EXTERNAL ACTION
   EXTERNAL NAME DSN5JSBT;

 CREATE FUNCTION SYSTOOLS.JSON_GET_POS_ARR_INDEX
   ( INJSON                 BLOB(16M)
   , QUERY                  VARCHAR(32672) FOR BIT DATA
   )
   RETURNS                  INTEGER
   SPECIFIC SYSTOOLS.JSON_GET_POS_ARR_INDEX
   LANGUAGE C
   PARAMETER STYLE SQL
   PARAMETER CCSID UNICODE
   WLM ENVIRONMENT DSNWLM_GENERAL
   RUN OPTIONS 'XPLINK(ON)'
   DBINFO
   PROGRAM TYPE SUB
   DISALLOW PARALLEL
   NO SQL
   DETERMINISTIC
   CALLED ON NULL INPUT
   NO EXTERNAL ACTION
   SCRATCHPAD 2048
   EXTERNAL NAME DSN5JSGP;

 CREATE FUNCTION SYSTOOLS.JSON_LEN
   ( INJSON                 BLOB(16M)
   , INELEM                 VARCHAR(2048)
   )
   RETURNS                  INTEGER
   SPECIFIC SYSTOOLS.JSON_LEN
   LANGUAGE C
   PARAMETER STYLE SQL
   PARAMETER CCSID UNICODE
   RUN OPTIONS 'XPLINK(ON)'
   NO SQL
   DETERMINISTIC
   DISALLOW PARALLEL
   DBINFO
   RETURNS NULL ON NULL INPUT
   NO EXTERNAL ACTION
   WLM ENVIRONMENT DSNWLM_GENERAL
   EXTERNAL NAME DSN5JSLN;

 CREATE FUNCTION SYSTOOLS.JSON_TABLE
   ( INJSON                 BLOB(16M)
   , INELEM                 VARCHAR(2048)
   , RETTYPE                VARCHAR(100)
   )
   RETURNS TABLE
   ( TYPE                   INTEGER
   , VALUE VARCHAR(2048)
   )
   SPECIFIC SYSTOOLS.JSON_TABLE
   LANGUAGE C
   PARAMETER STYLE SQL
   PARAMETER CCSID UNICODE
   WLM ENVIRONMENT DSNWLM_GENERAL
   RUN OPTIONS 'XPLINK(ON)'
   DBINFO
   PROGRAM TYPE SUB
   DISALLOW PARALLEL
   NO SQL
   DETERMINISTIC
   NO EXTERNAL ACTION
   SCRATCHPAD 2048
   EXTERNAL NAME DSN5JSTA;

 CREATE FUNCTION SYSTOOLS.JSON_TABLE_BINARY
   ( INJSON                 BLOB(16M)
   , INELEM                 VARCHAR(2048)
   , RETTYPE                VARCHAR(100)
   )
   RETURNS TABLE
   ( TYPE                   INTEGER
   , VALUE                  VARCHAR(2048) FOR BIT DATA
   )
   SPECIFIC SYSTOOLS.JSON_TABLE_BINARY
   LANGUAGE C
   PARAMETER STYLE SQL
   PARAMETER CCSID UNICODE
   WLM ENVIRONMENT DSNWLM_GENERAL
   RUN OPTIONS 'XPLINK(ON)'
   DBINFO
   PROGRAM TYPE SUB
   DISALLOW PARALLEL
   NO SQL
   DETERMINISTIC
   NO EXTERNAL ACTION
   SCRATCHPAD 2048
   EXTERNAL NAME DSN5JSTB;

 CREATE FUNCTION SYSTOOLS.JSON_TYPE
   ( INJSON                 BLOB(16M)
   , INELEM                 VARCHAR(2048)
   , MAXLENGTH              INTEGER
   )
   RETURNS                  INTEGER
   SPECIFIC SYSTOOLS.JSON_TYPE
   LANGUAGE C
   PARAMETER STYLE SQL
   PARAMETER CCSID UNICODE
   WLM ENVIRONMENT DSNWLM_GENERAL
   RUN OPTIONS 'XPLINK(ON)'
   DBINFO
   PROGRAM TYPE SUB
   DISALLOW PARALLEL
   NO SQL
   DETERMINISTIC
   RETURNS NULL ON NULL INPUT
   NO EXTERNAL ACTION
   EXTERNAL NAME DSN5JSTP;

 CREATE FUNCTION SYSTOOLS.JSON_UPDATE
   ( INJSON                 BLOB(16M)
   , OP                     VARCHAR(256)
   , INELEM                 VARCHAR(32672)
   , NEWVALUE               VARCHAR(32672) FOR BIT DATA
   , POS_ARR_INDEX INTEGER
   )
   RETURNS BLOB(16M)
   SPECIFIC SYSTOOLS.JSON_UPDATE
   LANGUAGE C
   PARAMETER STYLE SQL
   PARAMETER CCSID UNICODE
   WLM ENVIRONMENT DSNWLM_GENERAL
   RUN OPTIONS 'XPLINK(ON)'
   DBINFO
   PROGRAM TYPE SUB
   DISALLOW PARALLEL
   NO SQL
   DETERMINISTIC
   CALLED ON NULL INPUT
   NO EXTERNAL ACTION
   SCRATCHPAD 2048
   EXTERNAL NAME DSN5JSUP;

 CREATE FUNCTION SYSTOOLS.JSON2BSON
   ( INJSON                 CLOB(16M)
   )
   RETURNS                  BLOB(16M)
   SPECIFIC JSON2BSON
   LANGUAGE C
   PARAMETER STYLE SQL
   PARAMETER CCSID UNICODE
   NO SQL
   WLM ENVIRONMENT DSNWLM_GENERAL
   RUN OPTIONS 'XPLINK(ON)'
   PROGRAM TYPE SUB
   DETERMINISTIC
   DISALLOW PARALLEL
   DBINFO
   RETURNS NULL ON NULL INPUT
   NO EXTERNAL ACTION
   EXTERNAL NAME 'DSN5JSJB';

 --£SET TERMINATOR à

 CREATE PROCEDURE SYSTOOLS.REGSP
   ( IN    SOURCE_STRING    VARCHAR(32000)
   , IN    PATTERN          VARCHAR(32000)
   , IN    FLAGS            VARCHAR(4)
   , OUT   VT               INT
   )
   LANGUAGE SQL
   READS SQL DATA
   DISABLE DEBUG MODE
   BEGIN
     DECLARE V1 VARCHAR(32000);
     DECLARE C1 CURSOR FOR ST1;
     IF SOURCE_STRING IS NULL THEN
       SET VT = 0;
     ELSE
       SET V1
         = 'SELECT XMLCAST(XMLQUERY(''fn:matches($x, "';
       SET V1
         = V1 !! PATTERN !! '" , '''''
              !! FLAGS !!  ''''')''
           PASSING CAST(? AS VARCHAR(300))'
              !! ' AS "x") AS INT) FROM SYSIBM.SYSDUMMY1';
       PREPARE ST1 FROM V1;
       OPEN C1 USING SOURCE_STRING;
       FETCH C1 INTO VT;
       CLOSE C1;
     END IF;
     RETURN;
   ENDà

 CREATE FUNCTION SYSTOOLS.REG_MATCHES
   ( SOURCE_STRING          VARCHAR(32000)
   , PATTERN                VARCHAR(32000)
   , FLAGS                  VARCHAR(4)
   )
   RETURNS                  INTEGER
   SPECIFIC SYSTOOLS.REG_MATCHES
   LANGUAGE SQL
   READS SQL DATA
   DISABLE DEBUG MODE
   NO EXTERNAL ACTION
   DETERMINISTIC
   BEGIN
     DECLARE V1 INT;
     CALL SYSTOOLS.REGSP (SOURCE_STRING, PATTERN, FLAGS, V1);
     RETURN V1;
   ENDà

 CREATE FUNCTION SYSTOOLS.REG_MATCHES
   ( SOURCE_STRING          VARCHAR(32000)
   , PATTERN                VARCHAR(32000)
   )
   RETURNS                  INTEGER
   SPECIFIC SYSTOOLS.REG_MATCHES_NOFLAGS
   LANGUAGE SQL
   READS SQL DATA
   DISABLE DEBUG MODE
   NO EXTERNAL ACTION
   DETERMINISTIC
   BEGIN
     DECLARE V1 INT;
     CALL SYSTOOLS.REGSP (SOURCE_STRING, PATTERN, '', V1);
     RETURN V1;
   ENDà

 --£SET TERMINATOR ;

