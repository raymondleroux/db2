-- DROP SPECIFIC FUNCTION DB2MQ.DSN2RVC RESTRICT;
-- DROP SPECIFIC FUNCTION DB2MQ.DSN2RVC0 RESTRICT;
-- DROP SPECIFIC FUNCTION DB2MQ.DSN2RVC1 RESTRICT;
-- DROP SPECIFIC FUNCTION DB2MQ.DSN2RVC2 RESTRICT;
-- DROP SPECIFIC FUNCTION DB2MQ.DSN2RV0 RESTRICT;
-- DROP SPECIFIC FUNCTION DB2MQ.DSN2RV1 RESTRICT;
-- DROP SPECIFIC FUNCTION DB2MQ.DSN2RV2 RESTRICT;
-- DROP SPECIFIC FUNCTION DB2MQ.DSN2SD RESTRICT;
