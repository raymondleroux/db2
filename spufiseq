-----------------------------------------------------------------------
-- Create the Unified debugger for Java and SQL routines
-----------------------------------------------------------------------
 CREATE PROCEDURE DB2DEBUG.CREATE_SESSION
   ( IN    SESSIONID           INTEGER
   , IN    HOST                VARCHAR(128)
   , IN    PORT                INTEGER
   , OUT   RESULT              INTEGER
   )
   EXTERNAL NAME DSNACRSS
   LANGUAGE C
   PARAMETER STYLE GENERAL
   PARAMETER CCSID EBCDIC
   NULL CALL
   NO SQL
   NOT DETERMINISTIC
   NO COLLID
   STAY RESIDENT YES
   PROGRAM TYPE SUB
   WLM ENVIRONMENT DSNWLM_DEBUGGER
   SECURITY DB2 ;

 CREATE PROCEDURE DB2DEBUG.DEBUGGERLEVEL
   ( IN    REQUESTXML          VARCHAR(4000)
   , OUT   REPLYXML            VARCHAR(4000)
   )
   EXTERNAL NAME DSNAGTLV
   LANGUAGE C
   PARAMETER STYLE GENERAL
   PARAMETER CCSID EBCDIC
   NULL CALL
   NO SQL
   NOT DETERMINISTIC
   NO COLLID
   STAY RESIDENT YES
   PROGRAM TYPE SUB
   WLM ENVIRONMENT DSNWLM_DEBUGGER
   SECURITY DB2 ;

 CREATE PROCEDURE DB2DEBUG.DESTROY_SESSION
   ( IN    SESSIONID           INTEGER
   , IN    HOST                VARCHAR(128)
   , IN    PORT                INTEGER
   , OUT   RESULT              INTEGER
   )
   EXTERNAL NAME DSNADTSS
   LANGUAGE C
   PARAMETER STYLE GENERAL
   PARAMETER CCSID EBCDIC
   NULL CALL
   NO SQL
   NOT DETERMINISTIC
   NO COLLID
   STAY RESIDENT YES
   PROGRAM TYPE SUB
   WLM ENVIRONMENT DSNWLM_DEBUGGER
   SECURITY DB2 ;

 CREATE PROCEDURE DB2DEBUG.GET_REPORT
   ( IN    SESSIONID           INTEGER
   , IN    HOST                VARCHAR(128)
   , IN    PORT                INTEGER
   , OUT   XMLDATA             VARCHAR(32000)
   , OUT   BINDATA             VARCHAR(32000) FOR BIT DATA
   , IN    WAITTIME            INTEGER
   , OUT   MOREDATE            INTEGER
   , OUT   RESULT              INTEGER
   )
   EXTERNAL NAME DSNAGTRP
   LANGUAGE C
   PARAMETER STYLE GENERAL WITH NULLS
   PARAMETER CCSID EBCDIC
   NULL CALL
   NO SQL
   NOT DETERMINISTIC
   NO COLLID
   STAY RESIDENT YES
   PROGRAM TYPE SUB
   WLM ENVIRONMENT DSNWLM_DEBUGGER
   SECURITY DB2 ;

 CREATE PROCEDURE DB2DEBUG.LIST_SESSION
   ( IN    HOST                VARCHAR(128)
   , IN    PORT                INTEGER
   , OUT   XMLDATA             VARCHAR(32000)
   , OUT   RESULT              INTEGER
   )
   EXTERNAL NAME DSNALTSS
   LANGUAGE C
   PARAMETER STYLE GENERAL
   PARAMETER CCSID EBCDIC
   NULL CALL
   NO SQL
   NOT DETERMINISTIC
   NO COLLID
   STAY RESIDENT YES
   PROGRAM TYPE SUB
   WLM ENVIRONMENT DSNWLM_DEBUGGER
