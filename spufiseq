./ ADD NAME=SYSCOLDD
select                                                                  00001001
--*                                                                     00002002
  FREQUENCY                                            ,                00002102
  ';'                                                  ,                00002202
  STATSTIME                                            ,                00002402
  ';'                                                  ,                00002502
  IBMREQD                                              ,                00002602
  ';'                                                  ,                00002702
  PARTITION                                            ,                00002802
  ';'                                                  ,                00002902
  TBOWNER                                              ,                00003002
  ';'                                                  ,                00003102
  TBNAME                                               ,                00003202
  ';'                                                  ,                00003302
  NAME                                                 ,                00003402
  ';'                                                  ,                00003502
  COLVALUE                                             ,                00003602
  ';'                                                  ,                00003702
  TYPE                                                 ,                00003802
