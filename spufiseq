   COMMENT ON AMIDB2.EVENTS
      ( WORKID     IS 'INTERNAL IDENTIFIER',
        TASKID     IS 'INTERNAL IDENTIFIER',
        FUNCTION   IS 'UTILITY NAME',
        QUALIFIER  IS 'OBJECT QUALIFIER (DB, CREATOR)',
        OBJECT     IS 'OBJECT NAME',
        OBJTYPE    IS 'OBJECT TYPE',
        RETCODE    IS 'RETURN CODE FROM FUNCTION',
        TIMESTMP   IS 'TIME STAMP FOR EVENT',
        RUNFLAG    IS 'R=RESTART,S=STARTOVER,U=UNDO',
        ELAPTIME   IS 'EVENT ELAPSED TIME',
        CPUTIME    IS 'EVENT CPU TIME',
        DSN        IS 'DATA SET NAME FOR FUNCTION',
        PARMS      IS 'PARMS TO BE PASSED IN'
      )
*
   COMMENT ON AMIDB2.EXCEPTIONS2
     (QUALIFIER     IS 'OBJECT QUALIFIER (DB, OWNER)'               ,
      OBJECT        IS 'OBJECT NAME'                                ,
      PARTITION     IS 'OBJECT PARTITION'                           ,
      OBJTYPE       IS 'OBJECT TYPE'                                  ,
      EXCEPTION     IS 'EXCEPTION TYPE'                               ,
      CURRENT_VALUEF IS 'THE CURRENT VALUE OF THE EXCEPTION'          ,
     COMPARE_VALUEF IS 'THE COMPARED TO VALUE FOR THE TRIGGER, IF ANY',
      TRIGGER_VALUE IS 'THE TRIGGER VALUE FOR THE EXCEPTION.'         ,
      TIMESTMP      IS 'TIME STAMP FOR EXCEPTION'                     ,
      ACTION_TAKEN  IS 'ACTION TAKEN ON THE EXCEPTION.'               ,
      JCLDSN        IS
        'DATA SET CONTAINING GENERATED UTILITY JCL FOR THE EXCEPTIONS')
*
   COMMENT ON AMIDB2mDO_WORKIDS
      ( WORKID   IS 'ID NAME',
        TASKID   IS 'TASK ASSOCIATED WITH THIS WORKID',
        CREATEID IS 'CREATOR NAME',
        CREATED  IS 'DATE/TIME CREATED',
        UPDATEID IS 'UPDATOR NAME',
        UPDATED  IS 'DATE/TIME UPDATED',
        JCLDSN   IS 'DATA SET NAME TO BE USED',
        WLDSN    IS 'WORK LIST DATA SET NAME',
        STATUS   IS 'STATUS OF WORKID',
        DIAGDSN  IS 'DIAGNOSTIC DATA SET NAME'
      )
*

