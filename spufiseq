 CREATE FUNCTION SYSFUN.SYSTS_ENCRYPT
   (                           VARCHAR(32)
   ,                           VARCHAR(36)
   )
   RETURNS                     VARCHAR(256)
   SPECIFIC SYSFUN.SYSTS_ENCRYPT
   PARAMETER CCSID UNICODE
   LANGUAGE JAVA
   EXTERNAL NAME 'com.ibm.es.nuvo.util.CryptionUtil.encrypt'
   WLM ENVIRONMENT DSNWLM_JAVA
   NO EXTERNAL ACTION
   NO FINAL CALL
   PROGRAM TYPE SUB
   PARAMETER STYLE JAVA ;

-----------------------------------------------------------------------
-- Create the DB2 trace formatting routines
-----------------------------------------------------------------------
 CREATE PROCEDURE SYSPROC.DSNWSPM
   ( INOUT PA                  CHAR(8)      FOR BIT DATA
   , INOUT PB                  CHAR(8)      FOR BIT DATA
   , OUT   P1                  CHAR(16)
   , OUT   P2                  CHAR(16)
   , OUT   P3                  INTEGER
   , OUT   P4                  INTEGER
   , OUT   P5                  INTEGER
   , OUT   P6                  INTEGER
   , OUT   P7                  INTEGER
   )
   PARAMETER CCSID EBCDIC
   PROGRAM TYPE MAIN
   EXTERNAL NAME DSNWSPM
   COLLID DSNWSPM
   LANGUAGE ASSEMBLE
   RUN OPTIONS 'TRAP(ON),TERMTHDAC(UADUMP)'
   PARAMETER STYLE GENERAL
   WLM ENVIRONMENT DSNWLM_NUMTCB1
   COMMIT ON RETURN NO ;

 CREATE PROCEDURE SYSPROC.DSNWZP
   ( OUT P10                   VARCHAR(32000)
   )
   PARAMETER CCSID EBCDIC
   PROGRAM TYPE MAIN
   EXTERNAL NAME DSNWZP
   COLLID DSNWZP
   LANGUAGE ASSEMBLE
   READS SQL DATA
   RUN OPTIONS 'TRAP(ON),TERMTHDAC(UADUMP)'
   PARAMETER STYLE GENERAL
   WLM ENVIRONMENT DSNWLM_NUMTCB1
   COMMIT ON RETURN NO ;

